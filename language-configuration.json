{
  "comments": {
    "lineComment": "//",
    "blockComment": ["----------", "----------"]
  },
  "brackets": [
    ["tset", "tend"],
    ["ttitle=", "ttitle-end"],
    ["tstart=", "tend"]
  ],
  "autoClosingPairs": [
    { "open": "tset", "close": "tend" },
    { "open": "ttitle=", "close": "ttitle-end" },
    { "open": "tstart=", "close": "tend" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" },
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "<", "close": ">" },
    { "open": "“", "close": "”" },
    { "open": "‘", "close": "’" },
    { "open": "（", "close": "）" },
    { "open": "【", "close": "】" },
    { "open": "《", "close": "》" }
  ],
  "surroundingPairs": [
    ["tset", "tend"],
    ["ttitle=", "ttitle-end"],
    ["tstart=", "tend"],
    ["\"", "\""],
    ["'", "'"],
    ["(", ")"],
    ["[", "]"],
    ["{", "}"],
    ["<", ">"],
    ["“", "”"],
    ["‘", "’"],
    ["（", "）"],
    ["【", "】"],
    ["《", "》"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(tset|ttitle=.*|\\d+\\s+tstart=.*)\\s*$",
      "end": "^\\s*(tend|ttitle-end)\\s*$"
    }
  },
  "wordPattern": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b|\\b\\d+\\b|\\b[0-9A-Fa-f]{2}\\b",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(tset|ttitle=.*|\\d+\\s+tstart=.*)\\s*$",
    "decreaseIndentPattern": "^\\s*(tend|ttitle-end)\\s*$"
  }
}
